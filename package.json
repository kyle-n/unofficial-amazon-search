{
  "name": "unofficial-amazon-search",
  "version": "2.0.1",
  "description": "A simple client for searching Amazon",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "compile": "tsc && webpack",
    "clean": "shx rm -rf _bundles lib",
    "build": "npm run clean && npm run compile",
    "prepare": "npm run compile",
    "preview-npm-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
  },
  "author": "Kyle Nazario",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyle-n/unofficial-amazon-search.git"
  },
  "keywords": [
    "amazon",
    "amazon products",
    "amazon api",
    "node",
    "javascript",
    "web scraping",
    "scraping"
  ],
  "bugs": {
    "url": "https://github.com/kyle-n/unofficial-amazon-search/issues"
  },
  "files": [
    "lib/**/*",
    "_bundles/**/*"
  ],
  "homepage": "https://github.com/kyle-n/unofficial-amazon-search#readme",
  "dependencies": {
    "browser-or-node": "^1.3.0",
    "isomorphic-fetch": "^3.0.0",
    "jsdom": "^16.4.0"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/browser-or-node": "^1.2.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^26.0.15",
    "@types/jsdom": "^16.2.5",
    "@types/node": "^13.13.5",
    "jest": "^26.6.3",
    "path": "^0.12.7",
    "shx": "^0.3.3",
    "source-map-loader": "^1.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
